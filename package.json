{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "format": "prettier --write .",
    "start": "react-native start",
    "test": "jest",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "clean": "rm -rf node_modules && rm -rf yarn.lock",
    "clean:android": "cd android && ./gradlew clean && ./gradlew build && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "storybook": "react-native start",
    "storybook-generate": "sb-rn-get-stories",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
